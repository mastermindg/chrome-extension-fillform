{
  "manifest_version": 2,
  "name": "Form Filler",
  "description": "This extension will populate the selected form field on a page",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png",
   "default_title": "Fill in the form"
  },
  "background": {
        "page": "pages/background.html",
        "persistent": false
  },
  "permissions": [
   "contextMenus",
   "activeTab",
   "storage"
  ]
}
