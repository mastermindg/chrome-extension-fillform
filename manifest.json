{
  "manifest_version": 2,
  "name": "Form Filler",
  "description": "This extension will populate the selected form field on a page",
  "version": "1.0",
  "browser_action": {
      "default_title": "Set your form.",
      "default_icon": "icon.png",
      "default_popup": "pages/popup.html"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "tabs", "http://*/*", "https://*/*"
  ]
}
